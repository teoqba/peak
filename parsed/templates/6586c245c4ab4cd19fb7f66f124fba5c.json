{"components/common/forms/checkbox":"\n<div>\n  <label for=\"{{id}}\">{{label}}</label>\n  {{#if checkboxes}}\n    {{#each checkboxes}}\n      <li>\n        <input type=\"checkbox\" value=\"{{value}}\" name=\"{{name}}\" id=\"{{id}}\" class=\"{{class}}\" {{#if style}}style=\"{{style}}\" {{/if}}>\n        <label class=\"{{class}}\" for=\"{{label}}\">{{label}}</label>\n      </li>\n    {{/each}}\n  {{/if}}\n</div>\n"}