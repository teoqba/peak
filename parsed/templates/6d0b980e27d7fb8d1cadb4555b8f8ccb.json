{"components/products/listing-utils":"<div class=\"listing-utils\">\n  {{> components/products/sort}}\n\n  <div class=\"button-group\">\n    <button class=\"button-group-item view-mode-toggle {{#if theme_settings.collections-view-mode '===' 'grid'}}active{{/if}}\" data-listing-view=\"grid\">\n      {{> svg/grid-view}}\n    </button>\n    <button class=\"button-group-item view-mode-toggle {{#if theme_settings.collections-view-mode '===' 'list'}}active{{/if}}\" data-listing-view=\"list\">\n      {{> svg/list-view}}\n    </button>\n  </div>\n</div>\n","components/products/sort":"<div class=\"sorting\">\n  <form class=\"product-sort-select\" method=\"get\" data-sort-by>\n    <div class=\"form-field form-inline\">\n        <label class=\"form-label\" for=\"sort\">{{lang 'common.sorter.sort_by'}}</label>\n        <div class=\"form-select-wrapper\">\n          <select class=\"form-input form-select form-input-short dyna-width\" name=\"sort\" id=\"sort\">\n            <option value=\"featured\" {{#compare sort 'featured' operator='==='}}selected{{/compare}}>{{lang 'common.sorter.featured'}}</option>\n            <option value=\"newest\" {{#compare sort 'newest' operator='==='}}selected{{/compare}}>{{lang 'common.sorter.newest'}}</option>\n            <option value=\"bestselling\" {{#compare sort 'bestselling' operator='==='}}selected{{/compare}}>{{lang 'common.sorter.top_sellers'}}</option>\n            <option value=\"alphaasc\" {{#compare sort 'alphaasc' operator='==='}}selected{{/compare}}>{{lang 'common.sorter.alpha_asc'}}</option>\n            <option value=\"alphadesc\" {{#compare sort 'alphadesc' operator='==='}}selected{{/compare}}>{{lang 'common.sorter.alpha_desc'}}</option>\n            <option value=\"avgcustomerreview\" {{#compare sort 'avgcustomerreview' operator='==='}}selected{{/compare}}>{{lang 'common.sorter.reviews'}}</option>\n            <option value=\"priceasc\" {{#compare sort 'priceasc' operator='==='}}selected{{/compare}}>{{lang 'common.sorter.price_asc'}}</option>\n            <option value=\"pricedesc\" {{#compare sort 'pricedesc' operator='==='}}selected{{/compare}}>{{lang 'common.sorter.price_desc'}}</option>\n          </select>\n        </div>\n    </div>\n  </form>\n</div>\n\n{{!TODO: Show # per page}}\n","svg/grid-view":"<svg width=\"16\" height=\"16\" viewBox=\"0 0 16 16\" xmlns=\"http://www.w3.org/2000/svg\"><title>grid icon</title><path d=\"M6.192 9H.808C.362 9 0 9.362 0 9.808v5.384c0 .446.362.808.808.808h5.384c.446 0 .808-.362.808-.808V9.808C7 9.362 6.638 9 6.192 9zm0-9H.808C.362 0 0 .362 0 .808v5.384C0 6.638.362 7 .808 7h5.384C6.638 7 7 6.638 7 6.192V.808C7 .362 6.638 0 6.192 0zm9 0H9.808C9.362 0 9 .362 9 .808v5.384c0 .446.362.808.808.808h5.384c.446 0 .808-.362.808-.808V.808C16 .362 15.638 0 15.192 0zm0 9H9.808C9.362 9 9 9.362 9 9.808v5.384c0 .446.362.808.808.808h5.384c.446 0 .808-.362.808-.808V9.808c0-.446-.362-.808-.808-.808z\"/></svg>\n","svg/list-view":"<svg width=\"16\" height=\"14\" viewBox=\"0 0 16 14\" xmlns=\"http://www.w3.org/2000/svg\"><title>list icon</title><path d=\"M14.957 12H1.043C.467 12 0 12.448 0 13s.467 1 1.043 1h13.914c.576 0 1.043-.448 1.043-1s-.467-1-1.043-1zm0-6H1.043C.467 6 0 6.448 0 7s.467 1 1.043 1h13.914C15.533 8 16 7.552 16 7s-.467-1-1.043-1zM1.043 2h13.914C15.533 2 16 1.552 16 1s-.467-1-1.043-1H1.044C.466 0 0 .448 0 1s.467 1 1.043 1z\"/></svg>\n"}