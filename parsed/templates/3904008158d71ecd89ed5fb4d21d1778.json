{"components/cart/content":"<div class=\"cart-table-header\">\n  <div class=\"cell cart-item-info\">{{lang 'cart.product_info'}}</div>\n  <div class=\"cell cart-item-options\">{{lang 'cart.options'}}</div>\n  <div class=\"cell cart-item-quantity\">{{lang 'cart.quantity'}}</div>\n  <div class=\"cell cart-item-price\">{{lang 'cart.price'}}</div>\n</div>\n\n<div class=\"cart-table\">\n  {{#each cart.items}}\n    <article class=\"cart-item\" data-cart-item data-item-id=\"{{id}}\">\n      <div class=\"cell cart-item-info\">\n        <div class=\"cart-item-info-wrap\">\n          <div class=\"cart-item-image\">\n            {{#if type '===' 'GiftCertificate'}}\n              <div class=\"gift-card-image\">\n                <svg version=\"1.1\" id=\"Layer_1\" xmlns=\"http://www.w3.org/2000/svg\" xmlns:xlink=\"http://www.w3.org/1999/xlink\" x=\"0px\" y=\"0px\"\n                   viewBox=\"0 0 100 81.9\" style=\"enable-background:new 0 0 100 81.9;\" xml:space=\"preserve\">\n                  <path style=\"fill:currentColor\" d=\"M77.4,0.3L5.8,21.1c-4.3,1.2-6.7,5.7-5.5,10l12.3,42.3c1.2,4.3,5.7,6.7,10,5.5l71.6-20.8\n                    c4.3-1.2,6.7-5.7,5.5-10L87.4,5.8C86.2,1.5,81.7-0.9,77.4,0.3z M84.1,6.8l2.6,8.8L48.1,26.8c-0.7-2.2-1.8-5.4-3.2-7.6\n                    c0,0-7.6,1.7-11,5.6l-2.2-7.6L78.4,3.6C80.8,2.9,83.4,4.3,84.1,6.8z M6.8,24.5l17.4-5.1l2.4,8.1c-5.1-0.3-11.4,3.9-11.4,3.9\n                    c0.4,1.5,1,3,1.6,4.5L6.2,39l-2.6-8.8C2.9,27.7,4.3,25.2,6.8,24.5z M15.9,72.5l-7.6-26l17.1-5c-0.8,2.5-2,5.5-3.6,9.1l5.2,9.4\n                    c0,0,2.5-7.1,4.7-14.7l7.3,25.3l-17.4,5.1C19.2,76.3,16.6,74.9,15.9,72.5z M93.3,54.8L46.5,68.4l-8.3-28.5C45.9,46,55,52.5,55,52.5\n                    L53,42c-3.4-2.3-6.2-4.3-8.4-6l44.3-12.9l7.6,26C97.1,51.5,95.7,54.1,93.3,54.8z\"/>\n                  <ellipse style=\"fill:currentColor\" cx=\"53.6\" cy=\"79.3\" rx=\"41.3\" ry=\"2.6\"/>\n                </svg>\n              </div>\n            {{else}}\n              <a href=\"{{url}}\">\n                <img src=\"{{getImage image 'thumb' (cdn ../theme_settings.default-product-image)}}\" alt=\"{{image.alt}}\" />\n              </a>\n            {{/if}}\n          </div>\n          <div class=\"cart-item-text\">\n            <div class=\"product-brand\">{{brand.name}}</div>\n            {{#if type '===' 'GiftCertificate'}}\n              <a class=\"product-name\" href=\"{{edit_url}}\">{{name}}</a>\n            {{else}}\n              <a class=\"product-name\" href=\"{{url}}\">{{name}}</a>\n            {{/if}}\n            <div class=\"cart-item-giftwrap\">\n              {{> components/cart/gift-wrap-item}}\n            </div>\n          </div>\n        </div>\n      </div>\n      <div class=\"cell cart-item-options\">\n        {{#each options}}\n          <div class=\"product-option\"><span class=\"cart-label\">{{name}}: </span>{{value}}</div>\n        {{/each}}\n      </div>\n      <div class=\"cell cart-item-quantity\">\n        <span class=\"cart-label\">{{lang 'cart.quantity'}}:</span>\n        {{> components/common/quantity}}\n      </div>\n      <div class=\"cell cart-item-price\">\n        <span class=\"cart-label\">{{lang 'cart.price'}}:</span>\n        <span class=\"{{#if total_discounted}}price-discounted{{/if}}\">{{total.formatted}}</span>\n        {{#if total_discounted}}\n          <span class=\"cart-item-discount\">{{total_discounted.formatted}}</span>\n        {{/if}}\n        <span class=\"link cart-item-update\" data-cart-item-update>\n          {{#if ../cart_icon '==' 'bag'}}\n            {{lang 'cart.update_bag'}}\n          {{else}}\n            {{lang 'cart.update_cart'}}\n          {{/if}}\n        </span>\n        <a class=\"cart-item-remove\" data-cart-item-remove href=\"{{remove_url}}\">\n          {{> svg/close}}\n        </a>\n      </div>\n    </article>\n  {{/each}}\n</div>\n","components/cart/gift-wrap-item":"{{inject 'removeGiftWrap' (lang 'cart.gift_wrapping.remove_confirm')}}\n\n{{#if show_gift_wrapping}}\n  <ul>\n    {{#if gift_wrapping}}\n      <li class=\"giftwrapping-detail\">\n        <span>{{lang 'cart.gift_wrapping.wrapping_type'}}</span>\n        <span>{{gift_wrapping.name}}</span>\n      </li>\n      {{#if gift_wrapping.message}}\n        <li class=\"giftwrapping-detail\">\n          <span>{{lang 'cart.gift_wrapping.gift_message'}}</span>\n          <span>{{gift_wrapping.message}}</span>\n        </li>\n      {{/if}}\n      <li>\n        <a class=\"link\" href=\"#\" data-item-giftwrap=\"{{id}}\">{{lang 'cart.gift_wrapping.change'}}</a>\n        {{lang 'cart.gift_wrapping.change_or'}}\n        <a class=\"link\" href=\"{{gift_wrapping.remove_url}}\" data-giftwrap-remove>\n          {{lang 'cart.gift_wrapping.remove'}}\n        </a>\n      </li>\n    {{else}}\n      <li><a class=\"link\" href=\"#\" data-item-giftwrap=\"{{id}}\">{{lang 'cart.gift_wrapping.add'}}</a></li>\n    {{/if}}\n  </ul>\n{{/if}}\n","components/common/quantity":"{{!\n  Quantity Widget with Increment/Decrement controls.\n\n  @param product\n         cart.item or product object\n}}\n\n<div class=\"quantity-control\" data-quantity-control=\"{{id}}\">\n  <button class=\"button-control-action\" data-quantity-control-action=\"decrement\">\n    <svg class=\"icon icon-minus\"><use xlink:href=\"#icon-minus\" /></svg>\n  </button>\n  <input\n    class=\"form-input quantity-input\"\n    type=\"number\"\n    name=\"qty{{#if product_id}}-{{id}}{{else}}[]{{/if}}\"\n    pattern=\"[0-9]*\"\n    value=\"{{#if quantity}}{{quantity}}{{else}}{{#if min_purchase_quantity}}{{min_purchase_quantity}}{{else}}1{{/if}}{{/if}}\"\n    min=\"{{#if min_purchase_quantity}}{{min_purchase_quantity}}{{else}}0{{/if}}\"\n    {{#if max_purchase_quantity}}max=\"{{max_purchase_quantity}}\"{{/if}}\n    data-quantity-control-input\n  >\n  <button class=\"button-control-action\" data-quantity-control-action=\"increment\">\n    <svg class=\"icon icon-plus\"><use xlink:href=\"#icon-plus\" /></svg>\n  </button>\n</div>\n","svg/close":"<svg class=\"icon-close\" width=\"15\" height=\"15\" viewBox=\"0 0 15 15\" xmlns=\"http://www.w3.org/2000/svg\"><title>close</title><path d=\"M15 1.73L13.52.25 7.89 5.88 2.26.25.778 1.73l5.63 5.63-5.63 5.63 1.482 1.482 5.63-5.63 5.63 5.63 1.48-1.48-5.63-5.63L15 1.73\"/></svg>\n"}