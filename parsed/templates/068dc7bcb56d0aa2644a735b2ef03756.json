{"components/products/customizations/select":"<div class=\"form-field {{#if required }}required{{/if}}\">\n  <label class=\"form-label\" for=\"customization_{{this.id}}_{{../id}}\">\n    <div class=\"form-label-text product-option-title\">\n      {{ display_name }}\n    </div>\n    <select class=\"form-input form-select\" id=\"customization_{{this.id}}_{{../id}}\" name=\"ProductFields[{{this.id}}]\">\n      {{#each values}}\n        <option value=\"{{this}}\">{{this}}</option>\n      {{/each}}\n    </select>\n  </label>\n</div>\n"}