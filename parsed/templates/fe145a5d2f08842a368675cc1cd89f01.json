{"components/products/ratings":"{{#for 1 5}}\n  <span class=\"{{#if ../this.rating '>=' $index}}full{{/if}}\">\n    <svg class=\"icon icon-star\"><use xlink:href=\"#icon-star\" /></svg>\n  </span>\n{{/for}}\n","components/products/review-item":"<article class=\"review-item {{#if index '>' 1}}hidden{{/if}}\">\n  <div class=\"review-meta\">\n    <div class=\"product-item-rating\">\n      {{> components/products/ratings rating=rating size=\"medium\"}}\n      <div class=\"review-item-score sr-only\"><span>{{rating}}</span>/5</div>\n    </div>\n    {{#if name.length}}\n      <span class=\"review-author\" content=\"{{name}}\">{{name}}</span>\n    {{/if}}\n    <span class=\"review-date\">{{date}}</span>\n  </div>\n\n  <h4 class=\"review-item-title\">{{title}}</h4>\n  <p class=\"review-item-body\">{{nl2br text}}</p>\n</article>\n"}
