{"components/products/product-single-images":"<div class=\"product-images-container\">\n  <div class=\"product-images-loader\">\n    <svg class=\"icon icon-spinner\"><use xlink:href=\"#icon-spinner\" /></svg>\n  </div>\n\n  <div class=\"product-slides-container\">\n    <div class=\"product-slides-wrap\">\n      {{#if images}}\n        {{#each images}}\n          <a class=\"product-image\" style=\"background-image:url({{getImage this 'zoom'}});\" href=\"{{getImage this 'zoom'}}\" data-product-image>\n            <img src=\"{{getImage this 'product'}}\" alt=\"{{alt}}\">\n          </a>\n        {{/each}}\n      {{else}}\n        <img src=\"{{getImage this 'product' (cdn default_image)}}\" alt=\"{{alt}}\">\n      {{/if}}\n    </div>\n  </div>\n\n  <div class=\"product-images-pagination\">\n    {{#each images}}\n      <div class=\"pagination-item\">\n        <img src=\"{{getImage this 'small-thumb'}}\" alt=\"{{this.alt}}\">\n      </div>\n    {{/each}}\n  </div>\n</div>\n"}