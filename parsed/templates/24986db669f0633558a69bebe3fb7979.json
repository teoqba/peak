{"components/common/forms/radio":"\n<div>\n  <ul aria-required=\"{{require}}\" class=\"{{class}}\">\n    {{#if radios}}\n      {{#each radios}}\n        <li>\n          <input type=\"radio\" {{#if name}}name=\"{{name}}\"{{/if}} id=\"{{id}}\" aria-required=\"{{required}}\" {{#if style}}style=\"{{style}}\"{{/if}}>\n          <label for=\"{{id}}\">{{label}}</label>\n        </li>\n      {{/each}}\n    {{/if}}\n  </ul>\n</div>\n"}