{"components/cart/coupon-codes":"{{inject 'couponCodeEmptyInput' (lang 'cart.coupon_codes.error')}}\n\n<div class=\"cart-totals-item coupon-code\" data-coupon-codes>\n  <form class=\"form form-coupon\" method=\"post\" action=\"{{urls.cart}}\" data-coupon-code-form>\n    <p class=\"cart-total-label coupon-code-title\">{{lang 'cart.coupon_codes.title'}}</p>\n    <div class=\"input-group\">\n      <div class=\"form-field input-group-item\">\n        <label class=\"form-label\" for=\"couponcode\">\n          <span class=\"form-label-text sr-only\">{{lang 'cart.coupon_codes.enter_code'}}</span>\n          <input class=\"form-input\" id=\"couponcode\" type=\"text\" name=\"couponcode\" value=\"\" placeholder=\"{{lang 'cart.coupon_codes.enter_code'}}\" data-coupon-code-input>\n        </label>\n      </div>\n      <div class=\"form-actions input-group-item\">\n        <input class=\"button button-tertiary\" type=\"submit\" value=\"{{lang 'cart.coupon_codes.submit'}}\">\n        <input type=\"hidden\" name=\"action\" value=\"applycoupon\">\n      </div>\n    </div>\n  </form>\n</div>\n"}