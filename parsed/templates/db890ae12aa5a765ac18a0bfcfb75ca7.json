{"components/common/forms/date":"\n<div>\n  <label for=\"{{id}}\" {{#if style}}style=\"{{style}}\" {{/if}}>{{label}}</label>\n  <div>\n    <select name=\"{{day.name}}\" id=\"{{id}}\" aria-required=\"{{required}}\">\n      {{#each day.items}}\n        <option {{#equals ../day.value value}} selected {{/equals}}value=\"{{value}}\">{{label}}</option>\n      {{/each}}\n    </select>\n\n    <select name=\"{{month.name}}\" id=\"{{id}}\"  aria-required=\"{{required}}\">\n      {{#each month.items}}\n        <option {{#equals ../month.value value}} selected {{/equals}}value=\"{{value}}\">{{label}}</option>\n      {{/each}}\n    </select>\n\n    <select name=\"{{year.name}}\" id=\"{{id}}\" aria-required=\"{{required}}\">\n      {{#each year.items}}\n        <option {{#equals ../year.value value}} selected {{/equals}}value=\"{{value}}\">{{label}}</option>\n      {{/each}}\n    </select>\n  </div>\n</div>\n"}