{"components/footer/footer":"<footer class=\"site-footer\">\n  <div class=\"container\">\n    <div class=\"footer-nav-wrap\">\n      <div class=\"footer-col footer-nav\">\n        <h4 class=\"footer-title\">{{lang 'footer.page_nav'}}</h4>\n        {{> components/common/navigation nav_items=pages}}\n      </div>\n      <div class=\"footer-col footer-nav\">\n        {{#if theme_settings.footer-show-categories}}\n          <h4 class=\"footer-title\">{{lang 'footer.category_nav'}}</h4>\n          {{> components/common/navigation-top-level nav_items=categories}}\n        {{else}}\n          {{> components/footer/brand-navigation}}\n        {{/if}}\n      </div>\n      {{#if theme_settings.footer-show-address}}\n        <div class=\"footer-col footer-info\">\n          <h4 class=\"footer-title\">{{lang 'footer.address'}}</h4>\n          <p>{{nl2br settings.address}}</p>\n          <p>{{settings.phone_number}}</p>\n        </div>\n      {{/if}}\n      {{#if social_media.length}}\n        <div class=\"footer-col footer-social\">\n          <h4 class=\"footer-title\">{{lang 'footer.social_title'}}</h4>\n          {{> components/common/social-media-links links=social_media}}\n        </div>\n      {{/if}}\n    </div>\n\n\n    <div class=\"bottom-bar {{#if theme_settings.theme-credits}}show-credits{{/if}}\">\n      <div class=\"footer-payment-icons\">\n        <ul class=\"footer-payment-icons-list\">\n          {{#if theme_settings.payment_visa}}\n            <li>{{> svg/visa}}</li>\n          {{/if}}\n\n          {{#if theme_settings.payment_mastercard}}\n            <li>{{> svg/mastercard}}</li>\n          {{/if}}\n\n          {{#if theme_settings.payment_american_express}}\n            <li>{{> svg/amex}}</li>\n          {{/if}}\n\n          {{#if theme_settings.payment_discover}}\n            <li>{{> svg/discover}}</li>\n          {{/if}}\n\n          {{#if theme_settings.payment_paypal}}\n            <li>{{> svg/paypal}}</li>\n          {{/if}}\n\n          {{#if theme_settings.payment_apple_pay}}\n            <li>{{> svg/applepay}}</li>\n          {{/if}}\n        </ul>\n      </div>\n\n      {{#if theme_settings.store-copyright}}\n        <span class=\"footer-site-copyright\">&copy; <script>document.write(new Date().getFullYear())</script> {{settings.store_name}}</span>\n      {{/if}}\n\n      {{#if theme_settings.theme-credits}}\n        <p class=\"footer-theme-credits\">Peak theme by <a href=\"https://pixelunion.net\" target=\"_blank\">Pixel Union</a>, powered by <a href=\"https://bigcommerce.com\" target=\"_blank\" rel=\"nofollow\">BigCommerce</a></p>\n      {{/if}}\n\n      {{#if theme_settings.sitemap}}\n        <p><a href=\"{{urls.sitemap}}\">{{lang 'sitemap.title'}}</a></p>\n      {{/if}}\n    </div>\n  </div>\n</footer>\n","components/common/navigation":"{{!\n  @param nav_items\n         > either \"pages\" or \"categories\", passed from parent template\n}}\n\n<nav class=\"navigation\">\n  <ul>\n    {{#each nav_items}}\n      <li class=\"parent {{#if children}}tier-dropdown{{/if}}\">\n        <a class=\"top-level-nav-link {{#if children}}tier-toggle{{/if}}\" href=\"{{url}}\">{{name}}</a>\n        {{#if children}}\n          <ul class=\"tier-panel\">\n            <li class=\"parent-all\"><a href=\"{{url}}\">{{lang 'common.navigation_all'}} {{name}}</a></li>\n            {{#each children}}\n              <li {{#if children}}class=\"tier-dropdown\"{{/if}}>\n                <a {{#if children}}class=\"tier-toggle\"{{/if}} href=\"{{url}}\">{{name}}</a>\n                {{#if children}}\n                  <ul class=\"tier-panel\">\n                    <li class=\"parent-all\"><a href=\"{{url}}\">{{lang 'common.navigation_all'}} {{name}}</a></li>\n                    {{#each children}}\n                      {{> components/common/site-navigation-submenus}}\n                    {{/each}}\n                  </ul>\n                {{/if}}\n              </li>\n            {{/each}}\n          </ul>\n        {{/if}}\n      </li>\n    {{/each}}\n    {{#if settings.gift_certificates_enabled}}\n      {{#if nav_items '==' pages}}\n        <li><a href=\"{{urls.gift_certificate.purchase}}\">{{lang 'header.gift_certificate_cta'}}</a></li>\n      {{/if}}\n    {{/if}}\n  </ul>\n</nav>\n","components/common/navigation-top-level":"{{!\n  @param nav_items\n         > either \"pages\" or \"categories\", passed from parent template\n}}\n\n<nav class=\"navigation\">\n  <ul>\n    {{#each nav_items}}\n      <li>\n        <a class=\"top-level-nav-link\" href=\"{{url}}\">\n          {{name}}\n        </a>\n      </li>\n    {{/each}}\n  </ul>\n</nav>\n","components/footer/brand-navigation":"{{#if shop_by_brand}}\n  <h4 class=\"footer-title\">{{lang 'footer.brand_nav'}}</h4>\n  <nav class=\"navigation\">\n    <ul>\n      {{#each shop_by_brand}}\n        {{#if @index '<' 5}}\n          <li>\n            <a href=\"{{url}}\">{{name}}</a>\n          </li>\n        {{/if}}\n      {{/each}}\n      {{#if shop_by_brand.length '>' 5}}\n        <li class=\"footer-link-separation\">\n          <a href=\"{{urls.brands}}\">{{lang 'footer.view_all'}}</a>\n        </li>\n      {{/if}}\n    </ul>\n  </nav>\n{{/if}}\n","components/common/social-media-links":"<ul class=\"footer-social-media\">\n  {{#each links}}\n    {{#if display_name '==' 'twitter'}}\n      <li class=\"social-media-link\">\n        <a href=\"{{this.url}}\" title=\"{{this.name}}\" target=\"_blank\">\n          {{> svg/social/twitter}}\n          <span class=\"sr-only\">{{this.name}}</span>\n        </a>\n      </li>\n    {{/if}}\n\n    {{#if display_name '==' 'facebook'}}\n      <li class=\"social-media-link\">\n        <a href=\"{{this.url}}\" title=\"{{this.name}}\" target=\"_blank\">\n          {{> svg/social/facebook}}\n          <span class=\"sr-only\">{{this.name}}</span>\n        </a>\n      </li>\n    {{/if}}\n\n    {{#if display_name '==' 'google+'}}\n      <li class=\"social-media-link\">\n        <a href=\"{{this.url}}\" title=\"{{this.name}}\" target=\"_blank\">\n          {{> svg/social/googleplus}}\n          <span class=\"sr-only\">{{this.name}}</span>\n        </a>\n      </li>\n    {{/if}}\n\n    {{#if display_name '==' 'pinterest'}}\n      <li class=\"social-media-link\">\n        <a href=\"{{this.url}}\" title=\"{{this.name}}\" target=\"_blank\">\n          {{> svg/social/pinterest}}\n          <span class=\"sr-only\">{{this.name}}</span>\n        </a>\n      </li>\n    {{/if}}\n\n    {{#if display_name '==' 'youtube'}}\n      <li class=\"social-media-link\">\n        <a href=\"{{this.url}}\" title=\"{{this.name}}\" target=\"_blank\">\n          {{> svg/social/youtube}}\n          <span class=\"sr-only\">{{this.name}}</span>\n        </a>\n      </li>\n    {{/if}}\n\n    {{#if display_name '==' 'instagram'}}\n      <li class=\"social-media-link\">\n        <a href=\"{{this.url}}\" title=\"{{this.name}}\" target=\"_blank\">\n          {{> svg/social/instagram}}\n          <span class=\"sr-only\">{{this.name}}</span>\n        </a>\n      </li>\n    {{/if}}\n\n    {{#if display_name '==' 'linkedin'}}\n      <li class=\"social-media-link\">\n        <a href=\"{{this.url}}\" title=\"{{this.name}}\" target=\"_blank\">\n          {{> svg/social/linkedin}}\n          <span class=\"sr-only\">{{this.name}}</span>\n        </a>\n      </li>\n    {{/if}}\n\n    {{#if display_name '==' 'tumblr'}}\n      <li class=\"social-media-link\">\n        <a href=\"{{this.url}}\" title=\"{{this.name}}\" target=\"_blank\">\n          {{> svg/social/tumblr}}\n          <span class=\"sr-only\">{{this.name}}</span>\n        </a>\n      </li>\n    {{/if}}\n\n    {{#if display_name '==' 'stumbleupon'}}\n      <li class=\"social-media-link\">\n        <a href=\"{{this.url}}\" title=\"{{this.name}}\" target=\"_blank\">\n          {{> svg/social/stumbleupon}}\n          <span class=\"sr-only\">{{this.name}}</span>\n        </a>\n      </li>\n    {{/if}}\n\n    {{#if display_name '==' 'rss'}}\n      <li class=\"social-media-link\">\n        <a href=\"{{this.url}}\" title=\"{{this.name}}\" target=\"_blank\">\n          {{> svg/social/rss}}\n          <span class=\"sr-only\">{{this.name}}</span>\n        </a>\n      </li>\n    {{/if}}\n  {{/each}}\n</ul>\n","svg/visa":"<svg xmlns=\"http://www.w3.org/2000/svg\" width=\"49\" height=\"32\" viewBox=\"0 0 49 32\" ><path d=\"M14.06 10.283h4.24l-6.303 15.472-4.236.003-3.258-12.33c2.318.953 4.38 3.023 5.22 5.276l.42 2.148zm3.356 15.488l2.503-15.5h4l-2.503 15.5h-4.002zm14.576-9.276c2.31 1.106 3.375 2.444 3.362 4.21C35.322 23.923 32.59 26 28.384 26c-1.796-.02-3.526-.394-4.46-.826l.56-3.47.516.247c1.316.58 2.167.815 3.77.815 1.15 0 2.384-.476 2.395-1.514.007-.678-.517-1.164-2.077-1.923-1.518-.74-3.53-1.983-3.505-4.21.024-3.013 2.81-5.117 6.765-5.117 1.55 0 2.795.34 3.586.65l-.542 3.36-.36-.177c-.74-.314-1.686-.617-2.994-.595-1.568 0-2.293.69-2.293 1.333-.01.73.848 1.205 2.246 1.924zm14.207-6.21l3.238 15.49h-3.714s-.368-1.78-.488-2.32c-.583 0-4.667-.01-5.125-.01-.155.42-.84 2.332-.84 2.332h-4.205L41.01 11.57c.418-1.01 1.137-1.284 2.096-1.284H46.2zm-4.937 9.99h3.322l-.927-4.474-.27-1.337c-.202.58-.554 1.52-.53 1.48 0 0-1.263 3.44-1.595 4.33zm-31.54-1.572C8.075 14.13 4.44 11.712 0 10.592l.053-.32h6.453c.87.033 1.573.325 1.815 1.307z\"/></svg>\n","svg/mastercard":"<svg xmlns=\"http://www.w3.org/2000/svg\" width=\"54\" height=\"32\" viewBox=\"0 0 54 32\" ><path d=\"M48.366 15.193c.6 0 .9.437.9 1.282 0 1.28-.546 2.21-1.337 2.21-.6 0-.9-.437-.9-1.31 0-1.282.572-2.183 1.336-2.183zm-10.09 3.082c0-.655.49-1.01 1.472-1.01.11 0 .19.028.382.028-.027.982-.545 1.636-1.227 1.636-.382 0-.628-.246-.628-.656zm-11.998-2.427v.327h-1.91c.165-.763.546-1.173 1.092-1.173.518 0 .818.3.818.845zM38.06.002c8.838 0 16.003 7.165 16.003 16.002s-7.165 16-16.003 16c-3.834 0-7.324-1.345-10.08-3.595 2.102-2.032 3.707-4.568 4.568-7.44h-1.33c-.833 2.552-2.297 4.806-4.2 6.626-1.89-1.816-3.34-4.078-4.17-6.62h-1.33c.857 2.856 2.434 5.4 4.52 7.432-2.75 2.22-6.223 3.594-10.036 3.594C7.165 32.002 0 24.84 0 16.