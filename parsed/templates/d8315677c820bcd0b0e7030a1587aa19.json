{"components/category/show-more":"{{#if category.facets}}\n  {{> components/faceted-search/show-more-facets category}}\n{{/if}}\n","components/faceted-search/show-more-facets":"<ul\n  id=\"faceted-search-option-column\"\n  class=\"facet-list-items faceted-search-option-columns\"\n  data-facet-filter-wrapper\n>\n  {{#each facets}}\n    {{#if facet '===' ../all_facets}}\n      {{#each items}}\n        <li class=\"facet-item nav-list-item\" data-facet-item>\n          <a\n            href=\"{{url}}\"\n            class=\"nav-list-action {{#if selected }}is-active{{/if}}\"\n            rel=\"nofollow\"\n            data-id=\"{{ id }}\"\n            data-faceted-search-facet\n          >\n            {{title}}\n            {{#if ../show_product_counts}}\n              <span data-facet-item-count>({{count}})</span>\n            {{/if}}\n          </a>\n        </li>\n      {{/each}}\n    {{/if}}\n  {{/each}}\n</ul>\n"}