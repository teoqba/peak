{"components/common/share-buttons":"{{#if share_buttons}}\n  <nav class=\"share-buttons-nav\">\n    <ul class=\"share-buttons\" data-share-buttons>\n\n      {{#each share_buttons.buttons}}\n        {{#if service '===' 'facebook'}}\n          <li class=\"social-link facebook\">\n            <a target=\"_blank\" href=\"http://www.facebook.com/sharer.php?u={{../../url}}\" class=\"facebook\">\n              {{> svg/social/facebook}}\n            </a>\n          </li>\n        {{/if}}\n\n        {{#if service '===' 'twitter'}}\n          <li class=\"social-link twitter\">\n            <a target=\"_blank\" href=\"https://twitter.com/share?url={{../../url}}&amp;text={{../../title}}\" class=\"twitter\">\n              {{> svg/social/twitter}}\n            </a>\n          </li>\n        {{/if}}\n\n        {{#if service '===' 'google_plusone'}}\n          <li class=\"social-link google-plus\">\n            <a target=\"_blank\" href=\"https://plus.google.com/share?url={{../../url}}\" class=\"google\">\n              {{> svg/social/googleplus}}\n            </a>\n          </li>\n        {{/if}}\n\n        {{#if service '===' 'linkedin'}}\n          <li class=\"social-link linkedin\">\n            <a target=\"_blank\" href=\"http://www.linkedin.com/shareArticle?url={{../../url}}&amp;title={{../../title}}\" class=\"linkedin\">\n              {{> svg/social/linkedin}}\n            </a>\n          </li>\n        {{/if}}\n\n        {{#if service '===' 'email'}}\n          <li class=\"social-link mail\">\n            <a href=\"mailto:?subject={{../../title}}&amp;body={{../../url}}\" class=\"email\">\n              {{> svg/social/email}}\n            </a>\n          </li>\n        {{/if}}\n\n        {{#if service '===' 'print'}}\n          <li class=\"social-link print\">\n            <a class=\"print\" href=\"javascript:;\" onclick=\"window.print()\">\n              {{> svg/social/print}}\n            </a>\n          </li>\n        {{/if}}\n\n        {{#if service '===' 'pinterest'}}\n          <li class=\"social-link pinterest\">\n            <a target=\"_blank\" href=\"//pinterest.com/pin/create/button/?url={{../../url}}&amp;media={{../../image}}&amp;description={{../../title}}\" class=\"share-pinterest\">\n              {{> svg/social/pinterest}}\n            </a>\n          </li>\n        {{/if}}\n      {{/each}}\n\n      {{#each share_buttons.buttons}}\n        {{#if service '===' 'facebook_like'}}\n          <li class=\"social-link facebook-like\">\n            <div class=\"fb-like\"\n              data-href=\"{{../../url}}\"\n              data-layout=\"button_count\"\n              data-action=\"like\"\n              data-show-faces=\"false\">\n            </div>\n          </li>\n        {{/if}}\n      {{/each}}\n\n    </ul>\n  </nav>\n{{/if}}\n","components/products/add-to-cart-form":"<form\n  class=\"form add-to-cart-form\"\n  method=\"post\"\n  action=\"{{product.cart_url}}\"\n  enctype=\"multipart/form-data\"\n  data-cart-item-add\n  data-product-options-count=\"{{product.options.length}}\"\n>\n  <input type=\"hidden\" name=\"action\" value=\"add\">\n  <input type=\"hidden\" name=\"product_id\" value=\"{{product.id}}\" data-product-id/>\n  <div class=\"wb-step-label\"> Step 1: Select Rim </div>\n\n  <div class=\"product-options-container\" data-product-option-change>\n    {{#if product.customizations}}\n      {{#each product.customizations}}\n        {{{dynamicComponent 'core/forms/customizations'}}}\n      {{/each}}\n    {{/if}}\n\n    {{#if product.options}}\n      {{#each product.options}}\n        {{{dynamicComponent 'core/forms/options'}}}\n      {{/each}}\n      <div class=\"wishlist-form\">\n        <button type=\"button\" style=\"display:none\" class=\"button button-wide wb-button wb-next-button\">Forward To Hub Selection</button>\n        <button type=\"button\" style=\"display:none\" class=\"button button-wide wb-button wb-back-button\">Back To Rim Selection</button>\n        <button type=\"button\" class=\"button button-secondary button-wide wb-button wb-start-over-button\">Start Over</button>\n      </div>\n    {{/if}}\n  </div>\n\n  {{#if product.event_date}}\n    {{> core/products/event}}\n  {{/if}}\n\n  <div class=\"product-quantity-submit\">\n    <div class=\"product-quantity-container\">\n      {{#if product.show_quantity_input}}\n        <div class=\"form-field form-inline\">\n          <label class=\"form-label\">\n            <strong class=\"form-label-text\">{{ lang 'product.quantity'}}</strong>\n            <input\n              type=\"text\"\n              class=\"product-quantity form-input\"\n              name=\"qty[]\"\n              value=\"{{#if product.quantity}}{{product.quantity}}{{else}}{{#if product.min_purchase_quantity}}{{product.min_purchase_quantity}}{{else}}1{{/if}}{{/if}}\"\n              min=\"{{#if product.min_purchase_quantity}}{{product.min_purchase_quantity}}{{else}}0{{/if}}\"\n              {{#if product.max_purchase_quantity}}max=\"{{product.max_purchase_quantity}}\"{{/if}}\n              pattern=\"[0-9]+\"\n            >\n            <div class=\"product-quantity-toggle-wrapper\">\n              <span class=\"product-quantity-toggle product-quantity-increment\">\n                <svg class=\"icon icon-plus\"><use xlink:href=\"#icon-plus\" /></svg>\n              </span>\n              <span class=\"product-quantity-toggle product-quantity-decrement\">\n                <svg class=\"icon icon-minus\"><use xlink:href=\"#icon-minus\" /></svg>\n              </span>\n            </div>\n          </label>\n        </div>\n      {{/if}}\n\n      <div class=\"share-block\">\n        <span class=\"form-label-text share-title\">{{ lang 'product.share'}}</span>\n        {{> components/common/share-buttons\n          share_buttons=settings.add_this\n          url=product.url\n          image=(getImage product.main_image 'thumb')\n          title=product.title\n        }}\n      </div>\n    </div>\n\n    {{#if product.can_purchase}}\n      <button\n        type=\"submit\"\n        class=\"button button-primary button-wide add-to-cart button-progress spinner\"\n        data-button-purchase\n      >\n        <span class=\"button-text\">\n          {{#if product.pre_order}}\n            {{lang 'product.pre_order'}}\n          {{else}}\n            {{#if cart_icon '==' 'bag'}}\n              {{lang 'product.add_to_bag'}}\n            {{else}}\n              {{lang 'product.add_to_cart'}}\n            {{/if}}\n          {{/if}}\n        </span>\n      </button>\n    {{else}}\n      {{#if product.out_of_stock}}\n        <div class=\"button button-primary button-wide add-to-cart button-disabled\">\n          {{lang 'product.sold_out'}}\n        </div>\n      {{/if}}\n    {{/if}}\n\n    {{#if settings.show_wishlist}}\n      <div class=\"wishlist-form\" data-wishlist-dropdown>\n        {{#if customer.wishlists.length}}\n          <a class=\"button button-secondary button-wide add-to-wishlist\" href=\"#\" data-wishlist-toggle>\n            {{lang 'product.add_to_wishlist'}}\n          </a>\n          <ul class=\"add-to-wishlists\" data-wishlist-panel>\n            {{#each customer.wishlists}}\n              <li><a href=\"{{../product.add_to_wishlist_url}}&wishlistid={{id}}\" data-wishlist=\"{{view_url}}\">{{name}} ({{num_items}})</a></li>\n            {{/each}}\n          </ul>\n        {{else}}\n          <a class=\"button button-secondary button-wide add-to-wishlist\" href=\"{{product.add_to_wishlist_url}}\">{{lang 'product.add_to_wishlist'}}</a>\n        {{/if}}\n      </div>\n    {{/if}}\n  </div>\n\n</form>\n","core/forms/customizations/checkbox":"\n<div class=\"form-field form-field-customizations form-field-checkbox{{#if required}} form-required{{/if}}\">\n  <div class=\"form-field-title\">\n    {{display_name}}\n    {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n  </div>\n  <label class=\"form-label\">\n    <span class=\"form-field-control\">\n      <input class=\"form-input form-checkbox\"\n        id=\"product-field-{{id}}\"\n        name=\"ProductFields[{{this.id}}]\"\n        type=\"checkbox\"\n        {{#if value}}checked{{/if}}\n        {{#if required}}required{{/if}}\n        aria-required=\"{{required}}\"\n      >\n      <span class=\"form-label-text product-customization-text\">\n        {{display_name}}\n      </span>\n    </span>\n  </label>\n</div>\n","core/forms/customizations/file":"\n<div class=\"form-field form-field-customizations form-field-file {{#if required}}form-required{{/if}}\">\n  <label class=\"form-label\">\n    <span class=\"form-field-title\">\n      {{display_name}}\n      {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n    </span>\n    <span class=\"form-field-control\">\n      <span class=\"form-file-wrapper\" data-button-label=\"{{lang 'core.account.products.upload'}}\">\n        <input\n          class=\"form-input form-file\"\n          id=\"product-fields-{{id}}\"\n          name=\"ProductFields[{{this.id}}]\"\n          type=\"file\"\n          {{#if required}}required{{/if}}\n          aria-required=\"{{required}}\">\n      </span>\n    </span>\n  </label>\n\n  {{#if file_types '!==' ''}}\n    <span class=\"form-file-types\">{{lang 'core.account.products.filetypes' file_types=file_types}}</span>\n  {{/if}}\n\n  <span class=\"form-file-size\">{{lang 'core.account.products.max_filesize' bytes=file_size}}</span>\n</div>\n","core/forms/customizations/select":"\n<div class=\"form-field form-field-customizations form-field-select{{#if required}} form-required{{/if}}\">\n  <label class=\"form-label\">\n    <span class=\"form-field-title\">\n      {{display_name}}\n      {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n    </span>\n    <span class=\"form-field-control\">\n      <span class=\"form-select-wrapper\">\n        <select\n          class=\"form-input input-select\"\n          id=\"product-fields-{{id}}\"\n          name=\"ProductFields[{{this.id}}]\"\n          {{#if required}}required{{/if}}\n          aria-required=\"{{required}}\">\n          <option value=\"\" {{#any values selected=true}}{{else}}selected{{/any}} disabled>{{lang 'product.pick_one'}}</option>\n          {{#each values}}\n            <option value=\"{{this}}\" {{#if selected}}selected{{/if}}>{{this}}</option>\n          {{/each}}\n        </select>\n      </span>\n    </span>\n  </label>\n</div>\n","core/forms/customizations/text":"\n<div class=\"form-field form-field-customizations form-field-text{{#if required}} form-required{{/if}}\">\n  <label class=\"form-label\" for=\"product-fields-{{id}}\">\n    <span class=\"form-field-title\">\n      {{display_name}}\n      {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n    </span>\n    <span class=\"form-field-control\">\n      <input class=\"form-input\"\n        id=\"product-fields-{{id}}\"\n        name=\"ProductFields[{{this.id}}]\"\n        type=\"text\"\n        placeholder=\"{{prefill}}\"\n        value=\"{{value}}\"\n        {{#if required}}required{{/if}}\n        aria-required=\"{{required}}\">\n    </span>\n  </label>\n</div>\n","core/forms/customizations/textarea":"\n<div class=\"form-field form-field-customizations form-field-textarea{{#if required}} form-required{{/if}}\">\n  <label class=\"form-label\">\n    <span class=\"form-field-title\">\n      {{display_name}}:\n      {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n    </span>\n    <span class=\"form-field-control\">\n      <textarea\n        class=\"form-input\"\n        id=\"product-fields-{{id}}\"\n        name=\"ProductFields[{{this.id}}]\"\n        rows=\"4\"\n        placeholder=\"{{prefill}}\"\n        {{#if required}}required{{/if}}\n        aria-required=\"{{required}}\"></textarea>\n    </span>\n  </label>\n</div>\n","core/forms/options/date":"<div class=\"form-field form-field-options form-field-date{{#if required}} form-required{{/if}}\"\n  data-product-attribute=\"date\">\n  <span class=\"form-label\">\n\n    <span class=\"form-field-title\">\n      {{display_name}}\n      {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n    </span>\n\n    <span class=\"form-field-control\">\n      <span class=\"form-select-wrapper form-date-year\">\n        <select class=\"form-input form-select\" name=\"attribute[{{id}}][year]\" {{#if required}}required{{/if}} aria-required=\"{{required}}\">\n          <option disabled>{{lang 'forms.date.year'}}</option>\n          {{#if earliest_year '===' latest_year}}\n            <option value=\"{{earliest_year}}\" selected=\"selected\">{{earliest_year}}</option>\n          {{else}}\n            {{#for earliest_year latest_year}}\n              <option value=\"{{$index}}\" {{#if ../selected_date.year '==' $index}}selected=\"selected\"{{/if}}>{{$index}}</option>\n            {{/for}}\n          {{/if}}\n        </select>\n      </span>\n\n      <span class=\"form-select-wrapper form-date-month\">\n        <select class=\"form-input form-select\" name=\"attribute[{{id}}][month]\" {{#if required}}required{{/if}} aria-required=\"{{required}}\">\n          <option disabled>{{lang 'forms.date.month'}}</option>\n          {{#for 1 12}}\n            <option value=\"{{$index}}\" {{#if ../selected_date.month '==' $index}}selected=\"selected\"{{/if}}>{{getShortMonth $index}}</option>\n          {{/for}}\n        </select>\n      </span>\n\n      <span class=\"form-select-wrapper form-date-day\">\n        <select class=\"form-input form-select\" name=\"attribute[{{id}}][day]\" {{#if required}}required{{/if}} aria-required=\"{{required}}\">\n          <option disabled>{{lang 'forms.date.day'}}</option>\n          {{#for 1 31}}\n            <option value=\"{{$index}}\" {{#if ../selected_date.day '==' $index}}selected=\"selected\"{{/if}}>{{$index}}</option>\n          {{/for}}\n        </select>\n        </span>\n    </span>\n\n  </span>\n</div>\n","core/forms/options/input-checkbox":"\n<div class=\"form-field form-field-options form-field-checkbox{{#if required}} form-required{{/if}}\"\n  data-product-attribute=\"input-checkbox\">\n  <div class=\"form-field-title\">\n    {{display_name}}\n    {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n  </div>\n\n  <div class=\"form-field-control\">\n    <label class=\"form-label {{class}}\">\n      <input\n        class=\"form-input form-checkbox\"\n        id=\"attribute-{{id}}\"\n        name=\"attribute[{{id}}]\"\n        type=\"checkbox\"\n        value=\"{{value}}\"\n        {{#if checked}}checked{{/if}}\n        {{#if required}}required{{/if}}\n        aria-required=\"{{required}}\">\n      <span class=\"form-label-text\">{{label}}</span>\n    </label>\n  </div>\n</div>\n","core/forms/options/input-file":"\n<div class=\"form-field form-field-options form-field-file {{#if required}}form-required{{/if}}\"\n  data-product-attribute=\"input-file\">\n  <label class=\"form-label\">\n    <span class=\"form-field-title\">\n      {{display_name}}\n      {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n    </span>\n    <span class=\"form-field-control\">\n      <span class=\"form-file-wrapper\" data-button-label=\"{{lang 'core.account.products.upload'}}\">\n        <input\n          class=\"form-input form-file\"\n          id=\"attribute-{{id}}\"\n          name=\"attribute[{{id}}]\"\n          type=\"file\"\n          {{#if required}}required{{/if}}\n          aria-required=\"{{required}}\">\n      </span>\n    </span>\n  </label>\n\n  {{#if file_types '!==' ''}}\n    <span class=\"form-file-types\">{{lang 'core.account.products.filetypes' file_types=file_types}}</span>\n  {{/if}}\n\n  <span class=\"form-file-size\">{{lang 'core.account.products.max_filesize' bytes=file_size}}</span>\n</div>\n","core/forms/options/input-numbers":"\n<div class=\"form-field form-field-options form-field-number{{#if required}} form-required{{/if}}\"\n data-product-attribute=\"input-number\">\n  <label class=\"form-label\">\n    <span class=\"form-field-title\">\n      {{display_name}}\n      {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n    </span>\n    <span class=\"form-field-control\">\n      <input class=\"form-input\" id=\"attribute-{{id}}\" name=\"attribute[{{id}}]\" type=\"number\" value=\"{{prefill}}\" {{#if required}}required{{/if}} aria-required=\"{{required}}\">\n    </span>\n  </label>\n</div>\n","core/forms/options/input-text":"\n<div class=\"form-field form-field-options form-field-text{{#if required}} form-required{{/if}}\"\n  data-product-attribute=\"input-text\">\n  <label class=\"form-label\">\n    <span class=\"form-field-title\">\n      <span class=\"wb-option-display-name\">{{display_name}}</span>\n      {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n    </span>\n    <span class=\"form-field-control\">\n      <input class=\"form-input\"\n        id=\"attribute-{{id}}\"\n        name=\"attribute[{{id}}]\"\n        type=\"text\"\n        placeholder=\"{{prefill}}\"\n        value=\"\"\n        {{#if required}}required{{/if}}\n        aria-required=\"{{required}}\">\n    </span>\n  </label>\n</div>\n","core/forms/options/product-list":"\n<div class=\"form-field form-field-options form-field-product-list{{#if required}} form-required{{/if}}\"\n  data-product-attribute=\"product-list\">\n  <div class=\"form-field-title\">\n    {{display_name}}\n    {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n  </div>\n\n  <div class=\"form-field-control\">\n    {{#unless values.0.image}}\n      {{!-- same as radio buttons if no image set --}}\n      {{#each values}}\n        <label class=\"form-label\" data-product-attribute-value=\"{{id}}\">\n          <input\n            class=\"form-input form-radio\"\n            name=\"attribute[{{../id}}]\"\n            type=\"radio\"\n            value=\"{{id}}\"\n            {{#if selected}}checked{{/if}}\n            {{#if ../required}}required{{/if}}\n            aria-required=\"{{required}}\">\n          <span class=\"form-label-text\">{{label}}</span>\n        </label>\n      {{/each}}\n      {{#unless required}}\n        <label class=\"form-label\">\n          <input\n            class=\"form-input form-radio\"\n            name=\"attribute[{{id}}]\"\n            type=\"radio\"\n            value=\"0\"\n            {{#if prefill '==' 0}}checked{{/if}}\n            {{#if ../required}}required{{/if}}\n            aria-required=\"{{required}}\">\n          <span class=\"form-label-text\">{{lang 'core.product.none'}}</span>\n        </label>\n      {{/unless}}\n    {{else}}\n      {{#each values}}\n        <label class=\"form-label product-picklist-item\" data-product-attribute-value=\"{{id}}\">\n          <input\n            class=\"form-input form-radio product-picklist-radio\"\n            name=\"attribute[{{../id}}]\"\n            type=\"radio\" value=\"{{id}}\"\n            {{#if selected}}checked{{/if}}\n            {{#if ../required}}required{{/if}}\n            aria-required=\"{{required}}\">\n          <span class=\"product-picklist-item-body\">\n            <span class=\"product-picklist-image-wrap\">\n              <img class=\"product-picklist-image\" src=\"{{getImage image 'core-product-picklist' (cdn ../theme_settings.default_image)}}\" alt=\"{{image.alt}}\">\n            </span>\n            <span class=\"product-picklist-label-text form-label-text\">{{label}}</span>\n          </span>\n        </label>\n      {{/each}}\n      {{#unless required}}\n        <label class=\"form-label product-picklist-item\">\n          <input\n            class=\"form-input form-radio product-picklist-radio\"\n            name=\"attribute[{{id}}]\"\n            type=\"radio\"\n            value=\"0\"\n            {{#if prefill '==' 0}}checked{{/if}}>\n          <span class=\"product-picklist-item-body picklist-none\">\n            <span class=\"product-picklist-label-text form-label-text\">{{lang 'product.none'}}</span>\n          </span>\n        </label>\n      {{/unless}}\n    {{/unless}}\n  </div>\n</div>\n","core/forms/options/set-radio":"\n<div class=\"form-field form-field-options form-field-radio{{#if required}} form-required{{/if}}\"\n  data-product-attribute=\"set-radio\">\n  <div class=\"form-field-title product-option-title\">\n    {{display_name}}\n    {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n  </div>\n  <div class=\"form-field-control\">\n    {{#each values}}\n      <label class=\"form-label\" data-product-attribute-value=\"{{id}}\">\n        <input\n          class=\"form-input form-radio\"\n          id=\"attribute-{{id}}\"\n          name=\"attribute[{{../id}}]\"\n          type=\"radio\"\n          value=\"{{id}}\"\n          {{#if selected}}checked{{/if}}\n          {{#if ../required}}required{{/if}}\n          aria-required=\"{{required}}\">\n        <span class=\"form-label-text\">{{label}}</span>\n      </label>\n    {{/each}}\n    {{#unless required}}\n      <label class=\"form-label\">\n        <input\n          class=\"form-input form-radio\"\n          name=\"attribute[{{../id}}]\"\n          type=\"radio\"\n          value=\"0\"\n          {{#if prefill '==' 0}}checked{{/if}}\n          aria-required=\"{{required}}\">\n        <span class=\"form-label-text\">{{lang 'core.product.none'}}</span>\n      </label>\n    {{/unless}}\n  </div>\n</div>\n","core/forms/options/set-rectangle":"\n<div class=\"form-field form-field-options form-field-rectangle{{#if required}} form-required{{/if}}\"\n  data-product-attribute=\"set-rectangle\">\n  <div class=\"form-field-title\">\n    <span class=\"wb-option-display-name\">{{display_name}}</span>\n    {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n  </div>\n  <div class=\"form-field-control\">\n    {{#each values}}\n      <label class=\"form-label rectangle\" data-product-attribute-value=\"{{id}}\">\n        <input\n          class=\"form-input form-rectangle\"\n          name=\"attribute[{{../id}}]\"\n          id=\"attribute-{{id}}\"\n          type=\"radio\"\n          value=\"{{id}}\"\n          {{#if selected}}checked{{/if}}\n          {{#if ../required}}required{{/if}}\n          aria-required=\"{{required}}\">\n        <span class=\"rectangle-text form-label-text\">{{label}}</span>\n      </label>\n    {{/each}}\n    {{#unless required}}\n      <label class=\"form-label rectangle\">\n        <input\n          class=\"form-input form-rectangle\"\n          name=\"attribute[{{../id}}]\"\n          type=\"radio\"\n          value=\"0\"\n          {{#if selected}}checked{{/if}}\n          aria-required=\"{{required}}\">\n        <span class=\"rectangle-text form-label-text\">{{lang 'core.product.none'}}</span>\n      </label>\n    {{/unless}}\n  </div>\n</div>\n","core/forms/options/set-select":"\n<div class=\"form-field form-field-options form-field-select{{#if required}} form-required{{/if}}\"\n  data-product-attribute=\"set-select\">\n  <label class=\"form-label\">\n    <span class=\"form-field-title\">\n      <span class=\"wb-option-display-name\">{{display_name}}</span>\n      {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n    </span>\n    <span class=\"form-field-control\">\n      <div class=\"form-select-wrapper\">\n        <select\n          class=\"form-input form-select\"\n          id=\"attribute-{{id}}\"\n          name=\"attribute[{{id}}]\"\n          {{#if ../required}}required{{/if}}\n          aria-required=\"{{required}}\">\n          <option value=\"\" class=\"wb-empty-option\" {{#any values selected=true}}{{else}}selected{{/any}}>{{lang 'product.pick_one'}}</option>\n          {{#each values}}\n            <option value=\"{{id}}\" class=\"wb-option\" data-wb-label=\"{{label}}\" {{#if selected}}selected{{/if}} data-product-attribute-value=\"{{id}}\">{{label}}</option>\n          {{/each}}\n          <!--{{#unless required}}-->\n            <!--<option value=\"{{id}}\" {{#if selected}}selected{{/if}} data-product-attribute-value=\"{{id}}\">{{lang 'core.product.none'}}</option>-->\n          <!--{{/unless}}-->\n        </select>\n      </div>\n    </span>\n  </label>\n  <button type=\"button\" class=\"wb-reset-button\" id=\"wb-reset-button-{{display_name}}\"><svg class=\"icon icon-close icon-wb\"><use xlink:href=\"#icon-close\" /></svg></button>\n</div>\n\n\n","core/forms/options/swatch":"\n<div class=\"form-field form-field-options form-field-swatch{{#if required}} form-required{{/if}}\"\n  data-swatch-selector\n  data-product-attribute=\"swatch\">\n  <div class=\"form-field-title\">\n    <span class=\"wb-option-display-name\">{{display_name}}</span>\n    <span class=\"swatch-value\" data-swatch-value>{{lang 'forms.swatches_none_selected'}}</span>\n    {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n  </div>\n\n  <div class=\"form-field-control\">\n    {{#each values}}\n      <label class=\"swatch-wrap\" data-swatch-value=\"{{label}}\" data-wb-swatch-color=\"{{data.[0]}}\" data-product-attribute-value=\"{{id}}\">\n        <span class=\"form-label-text\">{{label}}</span>\n\n        <input\n          class=\"form-input swatch-radio\"\n          id=\"attribute-{{id}}\"\n          type=\"radio\"\n          name=\"attribute[{{../id}}]\"\n          value=\"{{id}}\"\n          {{#if selected}}checked{{/if}}\n          {{#if ../required}}required{{/if}}\n          aria-required=\"{{required}}\">\n\n        <span class=\"swatch {{#if data.[1]}}two-colors{{/if}}{{#if data.[2]}} three-colors{{/if}}\">\n          {{#if pattern}}\n            <span class=\"swatch-color swatch-pattern\" style=\"background-image: url('{{getImage image 'core-swatch'}}');\"></span>\n          {{/if}}\n\n          {{#if data.[0]}}\n            <span class=\"swatch-color\" style=\"background-color: #{{data.[0]}}\"></span>\n          {{/if}}\n\n          {{#if data.[1]}}\n            <span class=\"swatch-color secondary\" style=\"background-color: #{{data.[1]}}\"></span>\n          {{/if}}\n\n          {{#if data.[2]}}\n            <span class=\"swatch-color tertiary\" style=\"background-color: #{{data.[2]}}\"></span>\n          {{/if}}\n        </span>\n\n        {{#if pattern}}\n          <span class=\"swatch-pattern-expanded\">\n            <img class=\"swatch-pattern-image\" src=\"{{getImage image 'core-swatch'}}\">\n          </span>\n        {{/if}}\n\n      </label>\n    {{/each}}\n    {{#unless required}}\n      <label class=\"swatch-wrap swatch-none\"  data-swatch-value=\"{{lang 'core.product.none'}}\" data-product-attribute-value=\"{{id}}\">\n        <span class=\"form-label-text\">{{lang 'core.product.none'}}</span>\n        <input\n          class=\"form-input swatch-radio\"\n          name=\"attribute[{{../id}}]\"\n          type=\"radio\"\n          value=\"0\"\n          {{#if prefill '==' 0}}checked{{/if}}\n          aria-required=\"{{required}}\"\n        >\n        <span class=\"swatch\">\n          <svg width=\"50\" height=\"50\" viewBox=\"0 0 50 50\" xmlns=\"http://www.w3.org/2000/svg\">\n            <path d=\"M46.717.5633l2.7197 2.7196L3.283 49.4366.5632 46.717\" fill-rule=\"nonzero\" fill=\"#D0021B\"/>\n          </svg>\n        </span>\n      </label>\n    {{/unless}}\n  </div>\n\n</div>\n","core/forms/options/textarea":"\n<div class=\"form-field form-field-options form-field-textarea{{#if required}} form-required{{/if}}\"\n  data-product-attribute=\"textarea\">\n  <label class=\"form-label\">\n    <span class=\"form-field-title\">\n      {{display_name}}\n      {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n    </span>\n    <span class=\"form-field-control\">\n      <textarea\n        class=\"form-input\"\n        id=\"attribute-{{id}}\"\n        name=\"attribute[{{id}}]\"\n        rows=\"4\"\n        placeholder=\"{{prefill}}\"\n        {{#if required}}required{{/if}}\n        aria-required=\"{{required}}\"></textarea>\n    </span>\n  </label>\n</div>\n","core/products/event":"<div class=\"form-field form-field-date form-required\">\n  <span class=\"form-label\">\n\n    <span class=\"form-field-title\">\n      {{product.event_date.name}}\n      <span class=\"required-text\">{{lang 'common.required'}}</span>\n    </span>\n\n    <span class=\"form-field-control\">\n      <span class=\"form-select-wrapper form-date-year\">\n        <select class=\"form-input form-select\" name=\"EventDate[Yr]\" required>\n          <option disabled selected>{{lang 'forms.date.year'}}</option>\n          {{#if product.event_date.earliest_year '==' product.event_date.latest_year}}\n            <option value=\"{{product.event_date.earliest_year}}\">{{product.event_date.earliest_year}}</option>\n          {{else}}\n            {{#for product.event_date.earliest_year product.event_date.latest_year}}\n              <option value=\"{{$index}}\">{{$index}}</option>\n            {{/for}}\n          {{/if}}\n        </select>\n      </span>\n\n      <span class=\"form-select-wrapper form-date-month\">\n        <select class=\"form-input form-select\" name=\"EventDate[Mth]\" required>\n          <option disabled selected>{{lang 'forms.date.month'}}</option>\n          {{#for 1 12}}\n            <option value=\"{{$index}}\">{{getShortMonth $index}}</option>\n          {{/for}}\n        </select>\n      </span>\n\n      <span class=\"form-select-wrapper form-date-day\">\n        <select class=\"form-input form-select\" name=\"EventDate[Day]\" required>\n          <option disabled selected>{{lang 'forms.date.day'}}</option>\n          {{#for 1 31}}\n            <option value=\"{{$index}}\">{{$index}}</option>\n          {{/for}}\n        </select>\n      </span>\n    </span>\n\n  </span>\n\n  {{#if product.event_date.date_start}}\n    {{#if product.event_date.date_end}}\n      <span class=\"product-event-bounds\">{{lang 'forms.date.bounds' start_date=product.event_date.date_start end_date=product.event_date.date_end}}</span>\n    {{/if}}\n  {{/if}}\n</div>\n","svg/social/email":"<svg width=\"19\" height=\"14\" viewBox=\"0 0 19 14\" xmlns=\"http://www.w3.org/2000/svg\"><title>email</title><path d=\"M17.5 0H1.167C.52 0 0 .52 0 1.167v11.666C0 13.48.52 14 1.167 14H17.5c.645 0 1.167-.52 1.167-1.167V1.167C18.667.52 18.145 0 17.5 0zm-1.167 2.333v.077l-7 5.445-7-5.445v-.077h14zm-14 9.334v-6.3l6.284 4.887c.21.164.463.246.716.246.253 0 .507-.082.717-.246l6.283-4.887v6.3h-14z\"/></svg>\n","svg/social/facebook":"<svg width=\"8\" height=\"16\" viewBox=\"0 0 8 16\" xmlns=\"http://www.w3.org/2000/svg\"><title>facebook</title><path d=\"M0 5.292h1.655V3.684c0-.71.018-1.803.532-2.48C2.73.487 3.474 0 4.755 0 6.842 0 7.72.297 7.72.297l-.412 2.45s-.69-.198-1.333-.198c-.644 0-1.22.23-1.22.872v1.87h2.638L7.21 7.685H4.754V16h-3.1V7.685H0V5.292\"/></svg>\n","svg/social/googleplus":"<svg width=\"21\" height=\"14\" viewBox=\"0 0 21 14\" xmlns=\"http://www.w3.org/2000/svg\"><title>google</title><path d=\"M6.668 5.523v2.79h3.595c-.573 1.804-1.46 2.79-3.595 2.79-2.188 0-3.907-1.86-3.907-4.158 0-2.297 1.72-4.156 3.908-4.156 1.146 0 1.928.437 2.605 1.038.573-.6.52-.656 1.875-2.023C9.95.655 8.388 0 6.668 0 2.968 0 0 3.117 0 7s2.97 7 6.668 7c5.522 0 6.877-5.03 6.408-8.422H6.668v-.055zm11.93.22V3.28h-1.615v2.462h-2.397v1.696h2.397v2.515h1.615V7.438h2.344V5.742h-2.344z\"/></svg>\n","svg/social/linkedin":"<svg width=\"18\" height=\"16\" viewBox=\"0 0 18 16\" xmlns=\"http://www.w3.org/2000/svg\"><title>linkedin</title><path d=\"M17.425 16h-3.85v-5.625c0-1.472-.603-2.477-1.928-2.477-1.013 0-1.577.678-1.84 1.332-.098.235-.082.56-.082.888V16H5.91s.05-9.964 0-10.87h3.815v1.707c.225-.746 1.444-1.81 3.39-1.81 2.414 0 4.31 1.563 4.31 4.93V16zM2.05 3.77h-.023C.797 3.77 0 2.94 0 1.888 0 .813.82 0 2.075 0c1.253 0 2.023